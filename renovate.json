{
    "extends": [
      "github>defenseunicorns/uds-common//config/renovate.json5",
      ":semanticCommitTypeAll(build)",
      "group:all",
      "replacements:all",
      "workarounds:all"
    ],
  "hostRules": [
    {
      "matchHost": "registry1.dso.mil",
      "hostType": "docker",
      "description": "Encrypted creds for registry1, scoped to this Github org using: https://github.com/renovatebot/renovate/blob/main/docs/usage/configuration-options.md#encrypted",
      "encrypted": {
        "username": "",
        "password": ""
      }
    }
  ],
  "packageRules": [
    {
      "groupName": "Gitlab Runner Support Dependencies",
      "labels": ["support-deps"],
      "commitMessageTopic": "dependency {{depName}} from currentValue: {{currentValue}} displayFrom: {{displayFrom}} currentVersion: {{currentVersion}} displayTo: {{displayTo}} newValue: {{newValue}} newVersion: {{newVersion}} version: {{version}}",
      "packagePatterns": ["*"]
    },
    {
      "groupName": "Gitlab Runner Package Dependencies",
      "labels": ["package-deps"],
      "commitMessageTopic": "dependency {{depName}} from currentValue: {{currentValue}} displayFrom: {{displayFrom}} currentVersion: {{currentVersion}} displayTo: {{displayTo}} newValue: {{newValue}} newVersion: {{newVersion}} version: {{version}}",
      "registryUrls": ["https://registry1.dso.mil"],
      "matchDatasources": ["docker"]
    },
    {
      "groupName": "Gitlab Runner Package Dependencies",
      "labels": ["package-deps"],
      "commitMessageTopic": "dependency {{depName}} from currentValue: {{currentValue}} displayFrom: {{displayFrom}} currentVersion: {{currentVersion}} displayTo: {{displayTo}} newValue: {{newValue}} newVersion: {{newVersion}} version: {{version}}",
      "matchDatasources": ["helm"]
    }
  ]
}